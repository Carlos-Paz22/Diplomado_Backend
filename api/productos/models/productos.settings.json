{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "name": "Productos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": false
    },
    "precio": {
      "type": "decimal",
      "required": false
    },
    "categorias": {
      "collection": "categorias",
      "via": "productos"
    },
    "user": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "productos",
      "dominant": true
    }
  }
}
